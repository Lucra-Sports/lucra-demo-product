{
  "name": "api",
  "version": "1.0.0",
  "description": "Simple Node and SQLite API with TypeScript and Prisma",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "test": "jest --testMatch='**/*.test.ts'",
    "test:watch": "jest --watch --testMatch='**/*.test.ts'",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run db:generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "postinstall": "npm run db:generate",
    "start:production": "npm run db:migrate:deploy && npm start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "express": "^4.18.2",
    "@aws-sdk/client-s3": "^3.633.0",
    "dotenv": "^16.3.1",
    "@prisma/client": "^5.6.0",
    "prisma": "^5.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
